pragma solidity >=0.8 <0.9.0;


contract countingCharacterOccurances {

//Counting occurrences of a certain character
    function countOccurance(string memory x , string memory y) public pure returns(uint){


        bytes memory z= bytes(x);
       bytes memory m= bytes(y);
        uint count=0;
        for(uint i=0;i<z.length;i++){
            
            if(z[i]==m[0]){
                count++;
            }

        }

        return  count;
    }
    
}
